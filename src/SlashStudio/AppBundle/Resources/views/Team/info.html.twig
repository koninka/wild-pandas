{% extends '::base.html.twig' %}

{% block headline %}
  {{ header }}
{% endblock %}

{% block content %}
  <p>{{ page.getText() | raw }}</p>
  <div>
    <h2>{{ 'navigation.video' | trans({}, 'navigation') }}</h2>
    {% for video in videos %}
      <div>{% media video, 'medium' %}</div>
    {% endfor %}
  </div>
{% endblock %}